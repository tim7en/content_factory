{
  "name": "ai-content-factory",
  "version": "1.0.0",
  "description": "An AI-powered content creation and monetization platform with automated niche detection and multi-platform publishing.",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t ai-content-factory .",
    "docker:run": "docker run -p 3000:3000 ai-content-factory",
    "migrate": "ts-node scripts/migrate-database.ts",
    "setup": "ts-node scripts/setup-apis.ts",
    "install:ci": "PUPPETEER_SKIP_DOWNLOAD=true npm install",
    "postinstall": "npm run setup || true"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "mongoose": "^7.6.3",
    "winston": "^3.11.0",
    "bull": "^4.12.2",
    "redis": "^4.6.10",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "googleapis": "^128.0.0",
    "openai": "^4.17.4",
    "ffmpeg-static": "^5.2.0",
    "sharp": "^0.32.6",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.2",
    "compromise": "^14.10.0",
    "sentiment": "^5.0.2"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.9",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/multer": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/cheerio": "^0.22.33",

    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "prettier": "^3.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.15"
  },
  "keywords": [
    "AI",
    "content creation",
    "monetization",
    "automation",
    "niche detection",
    "social media",
    "trends analysis",
    "avatar generation",
    "music generation",
    "youtube",
    "tiktok",
    "patreon"
  ],
  "author": "Content Factory Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/content-factory/ai-content-factory.git"
  },
  "bugs": {
    "url": "https://github.com/content-factory/ai-content-factory/issues"
  },
  "homepage": "https://github.com/content-factory/ai-content-factory#readme"
}